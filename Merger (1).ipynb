   "source": [
    "import PyPDF2\n",
    "\n",
    "pdfiles=[\"sample1.pdf\",\"sample2.pdf\"]\n",
    "merger=PyPDF2.PdfMerger()\n",
    "for filename in pdfiles:\n",
    "    pdfFile=open(filename,'rb')\n",
    "    pdfReader=PyPDF2.PdfReader(pdfFile)\n",
    "    merger.append(pdfReader)\n",
    "pdfFile.close()\n",
    "merger.write('merged.pdf')"
   ]
  }
